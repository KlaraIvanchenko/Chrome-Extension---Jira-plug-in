{
	"manifest_version": 2,

	"name": "JIRA",
	"version": "1.0",
	"description": "Work with JIRA tickets",

	"permissions": [
		"https://*/*",
		"http://*/*",
		"cookies"
	],

	"browser_action": {
		"default_title": "CodeSwat JIRA",
		"default_icon": "images/logo.png",
		"default_popup": "popup.html"
	}
}